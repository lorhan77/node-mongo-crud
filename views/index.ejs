<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% docs.forEach(function(clientes){ %>
        <li>
          <a href="/editar/<%= clientes._id %>">
            <%= clientes.nome %>
          </a>
        </li>
      <% }) %>
    </ul>
    <hr />
    <a href="/novo">Cadastrar novo cliente</a>
  </body>
</html>
